# Fittober Fitness Tracker Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=your_database_url_here

# Twilio WhatsApp Configuration
TWILIO_SID=your_twilio_sid_here
TWILIO_TOKEN=your_twilio_token_here
TWILIO_FROM=whatsapp:+your_twilio_number
TWILIO_TO=whatsapp:+your_personal_number

# Server Configuration
PORT=3001
NODE_ENV=development

# Optional: Custom gist URLs (comma-separated)
# GIST_URLS=https://gist.github.com/user/gist1,https://gist.github.com/user/gist2
